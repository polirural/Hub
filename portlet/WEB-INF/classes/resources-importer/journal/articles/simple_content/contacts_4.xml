<?xml version="1.0"?>

<root available-locales="en_US" default-locale="en_US">
	<dynamic-element name="content" type="text_area" index-type="keyword" index="0">
    	<dynamic-content language-id="en_US">
        	<![CDATA[
    <!-- Promo Block -->
    <section class="g-bg-gray-dark-v1">
      <div class="container g-color-white g-py-150">
        <h2 class="display-4 g-font-weight-600 text-uppercase">Our contacts</h2>
        <h3 class="h2 g-font-weight-300 mb-0">Got a question? We'd love to hear from you!</h3>
      </div>
    </section>
    <!-- End Promo Block -->

    <section class="clearfix">
      <div class="row no-gutters">
        <div class="col-md-6 align-self-center">
          <!-- Google Map -->
          <div id="GMapCustomized-light" class="js-g-map embed-responsive embed-responsive-21by9 g-height-100vh" data-type="custom" data-lat="40.674" data-lng="-73.946" data-zoom="12" data-title="Agency" data-styles='[["", "", [{"saturation":-100},{"lightness":51},{"visibility":"simplified"}]], ["", "labels", [{"visibility":"on"}]], ["water", "", [{"color":"#bac6cb"}]] ]'
          data-pin="true" data-pin-icon="[$FILE=pin_green.png$]">
          </div>
          <!-- End Google Map -->
        </div>

        <div class="col-md-6 align-self-center">
          <div class="g-pa-100">
            <div class="g-mb-60">
              <h2 class="h1 g-color-black">Contact us</h2>
              <p class="g-color-gray-dark-v3 g-font-size-16">We are a creative studio focusing on culture, luxury, editorial &amp; art. Somewhere between sophistication and simplicity.</p>
            </div>

            <div class="mb-5">
              <h2 class="h4 g-color-black">Pixeel Ltd.</h2>
              <p class="g-color-gray-dark-v3 g-font-size-16 mb-0">Unit 25 Suite 3, 925 Prospect PI</p>
              <p class="g-color-gray-dark-v3 g-font-size-16 mb-0">htmlstream@support.com</p>
            </div>

            <a class="btn u-btn-black g-font-weight-600 g-font-size-12 text-uppercase rounded0 g-py-15 g-py-13 g-px-40" href="#">Get in touch</a>
          </div>
        </div>
      </div>
    </section>



<script>
	(function loopMap() {
		setTimeout(function () {
			if($.HSCore != undefined && $.HSCore.components.HSTextSlideshow != undefined ){
        		// initialization of google map
				$.HSCore.components.HSGMap.init('.js-g-map');
		    }else{
			    loopMap()
		    }
		  }, 1000);
	}());
    
    function initMap() {
        $.HSCore.components.HSGMap.init('.js-g-map');
    }
    
    if (typeof google === 'object' && typeof google.maps === 'object') {
        initMap();
    } else {
        var script = document.createElement("script");
        script.type = "text/javascript";
        script.src = "http://maps.googleapis.com/maps/api/js?key=AIzaSyAtt1z99GtrHZt_IcnK-wryNsQ30A112J0&callback=initMap";
        document.body.appendChild(script);
    }
</script>

			]]>
		</dynamic-content>
	</dynamic-element>
</root>
            